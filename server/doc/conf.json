{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./utils",
      "./modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "exclude": [
      "./modules/static/token.json"
    ]
  },
  "plugins": [],
  "opts": {
    "template": "./node_modules/minami",
    "encoding": "utf8",
    "destination": "./doc/doc-page/",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}